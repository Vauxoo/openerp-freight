<openerp>
    <data>
 
        

        <record model="ir.ui.view" id="view_fleet_vehicle_service_stage_form_inherit">
            <field name="name">fleet.vehicle.log.services.form.inherit</field>
            <field name="model">fleet.vehicle.log.services</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="fleet.fleet_vehicle_log_services_form"/>
            <field name="arch" type="xml">
                <xpath expr="/form/sheet" position="before">
                    <header>
                      <field name="stage_id" widget="statusbar" clickable="True"/>
                    </header>
                </xpath>
                <field name="vendor_id" position="replace">
                    <field name="vendor_id" string="Workshop"/>
                </field>
            </field>
        </record>
        
    </data>
</openerp>
