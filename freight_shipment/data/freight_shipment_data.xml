<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="mt_fs_new" model="mail.message.subtype">
            <field name="name">Freight Shipment Order Created</field>
            <field name="res_model">freight.shipment</field>
            <field name="default" eval="False"/>
            <field name="description">Freight Shipment Order Created</field>
        </record>

        <record id="mt_fs_waiting" model="mail.message.subtype">
            <field name="name">Awaiting Freight Shipment Orde</field>
            <field name="res_model">freight.shipment</field>
            <field name="default" eval="False"/>
            <field name="description">Freight Shipment Order is Awaiting State</field>
        </record>

        <record id="mt_fs_exception" model="mail.message.subtype">
            <field name="name">Freight Shipment Order is in Exception State</field>
            <field name="res_model">freight.shipment</field>
            <field name="default" eval="False"/>
            <field name="description">Warning: The Freight Shipment is in Exception State</field>
        </record>

        <record id="mt_fs_vw_exception" model="mail.message.subtype">
            <field name="name">Freight Shipment Volumetric Weight Exception</field>
            <field name="res_model">freight.shipment</field>
            <field name="default" eval="False"/>
            <field name="description">Volumetric Weight Capacity Exceeded</field>
        </record>

        <record id="mt_fs_w_exception" model="mail.message.subtype">
            <field name="name">Freight Shipment Weight Capacity Exception</field>
            <field name="res_model">freight.shipment</field>
            <field name="default" eval="False"/>
            <field name="description">Weight Capacity Exceeded</field>
        </record>

        <record id="mt_fs_confirm" model="mail.message.subtype">
            <field name="name">Freight Shipment Confirmed</field>
            <field name="res_model">freight.shipment</field>
            <field name="default" eval="False"/>
            <field name="description">Freight Shipment Confirmed</field>
        </record>

        <record id="mt_fs_to_load" model="mail.message.subtype">
            <field name="name">Freight Shipment is in Process to be charged</field>
            <field name="res_model">freight.shipment</field>
            <field name="default" eval="False"/>
            <field name="description">Freight Shipment is in Process to be charged</field>
        </record>

        <record id="mt_fs_pending" model="mail.message.subtype">
            <field name="name">Freight Shipment change to Pending state</field>
            <field name="res_model">freight.shipment</field>
            <field name="default" eval="False"/>
            <field name="description">Freight Shipment is pending to be dispatch</field>
        </record>

    </data>
</openerp>
