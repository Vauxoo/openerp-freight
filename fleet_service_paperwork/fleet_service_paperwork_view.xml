<openerp>
    <data>
            <record model="ir.ui.view" id="view_fleet_service_type_form_inherit">
            <field name="name">fleet.vehicle.paperwork.form.inherit</field>
            <field name="model">fleet.vehicle</field>
            <field name="inherit_id" ref="fleet_special_equipment.view_fleet_vehicle_form_inherit"/>
            <field name="arch" type="xml">
                <xpath expr="/form/sheet/notebook/page[@string='Equipment']" position="after">
                    
                        <page string="Paperwork">
                            <field name="service_type_paperwork_ids" >
                                <tree editable="bottom">
                                    <field name="service_id" domain="[('category','=','paperwork')]"/>
                                    <field name="partner_id"/>
                                    <field name="date"/>
                                    <field name="status"/>
                                    <button name="send_expiration_message" string="Message" type="object" />
                                </tree>
                            </field>
                        </page>
                </xpath>
            </field>
        </record>   
        
        
    </data>
</openerp>
